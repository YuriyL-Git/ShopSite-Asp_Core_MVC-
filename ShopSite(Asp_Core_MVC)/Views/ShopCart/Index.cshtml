@model ShopCartModel
<div class="container">
    @foreach (var el in Model.ShopCart.ListShopCartItems)
    {
    <div class="alert alert-warning mt-3">
        <b>Автомобиль: </b> @el.car.Name<br />
        <b>Price:</b> @el.Price.ToString("C")
    </div>
    }
    <hr />
    <div class="btn btn-danger" asp-controller="Order">Pay</div>

</div>